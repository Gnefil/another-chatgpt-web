{
  "name": "another-chatgpt-web-service",
  "version": "0.0.1",
  "private": false,
  "description": "Another ChatGPT Web Service",
  "author": "Flat 2",
  "keywords": [
    "chatgpt",
    "openai-node",
    "express"
  ],
  "engines": {
    "node": "^18 || ^20"
  },
  "scripts": {
    "start": "esno ./src/index.ts",
    "dev": "esno watch ./src/index.ts",
    "prod": "node ./build/index.mjs",
    "build": "pnpm clean && tsup",
    "clean": "rimraf build",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "openai": "^4.52.2",
    "tiktoken": "^1.0.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.39",
    "esno": "^4.7.0",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}
